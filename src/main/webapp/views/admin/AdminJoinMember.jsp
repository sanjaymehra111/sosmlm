<html lang="en">

<head>

    <Title>New Member</Title>

    <!-- Bootstrap -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <!--For Favicon Logo-->
    <link rel="shortcut icon" href="/images/sosmlm_fevicon.png">

    <!-- Embed Button CSS -->
    <link rel="stylesheet" href="/style/joinMemberStyle.css">
    

</head>

<body>

<Section>

    <div class="Head">

        <div align="Center">
            <div class="col-md-3"></div>
            <div class="col-md-6">
                <div class="Head2">
                    <br>
                    <div style="background-color: rgb(255, 123, 0); padding: 10;">
                        <span style="color: white; text-transform: uppercase; font-size: 20px; font-weight: bold;">New Registration</span>
                    </div>
                    <br><br>
                    <div style="text-align: left;">
                        <div>Sponsor ID *</div>
                            <input class="InputType" type="text" maxlength="10">
                            <br><br>
                        <div>Sponsor Name *</div>
                            <input class="InputType" type="text">
                            <br><br>
                        <div>First Name *</div>
                            <input class="InputType" type="text" maxlength="50">
                            <br><br>
                        <div>Last Name *</div>
                            <input class="InputType" type="text" maxlength="50">
                            <br><br>
                        <div>Select Gender *</div>
                            Male &nbsp; <input name="Gender" class="RadioButton" type="radio"> &nbsp;&nbsp;&nbsp;&nbsp;
                            Female &nbsp; <input name="Gender" class="RadioButton" type="radio">
                            <br><br>
                        <div>Contact *</div>
                            <input class="InputType" type="number">
                            <br><br>
                        <div>Email ID *</div>
                            <input class="InputType" type="text" maxlength="100">
                            <br><br>
                        <div>PAN No*</div>
                            <input class="InputType" type="number">
                            <br><br>
                        <div>Password *</div>
                            <input class="InputType" type="password" maxlength="50">
                            <br><br>
                        <div>Re-Password *</div>
                            <input class="InputType" type="password" maxlength="50">
                            <br><br><br>

                        <button class="DefaultButton">Submit</button>
                        <br><br>

                        </div>
                        

            </div>
        </div>
        <div class="container-fluid"></div>
        <br><br>
        
    </div>
    
</Section>
  
</body>

</html>